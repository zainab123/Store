@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Viewcontact</title>
    @Html.Partial("PartialPage")
    @Html.Partial("AdminPartialHeader")

</head>

<body style="font-family:Verdana ;color:white;overflow-x:hidden ;background-image:url('/Content/images/nbg16.jpg');background-repeat:no-repeat;background-size:cover;">
    <div class="container-fluid">

        <h1> <b> <i> View FeedBack </i></b></h1>  <br /> <br />

        <div class="table-responsive">
            <table class="table" style="background:#ad5719; padding: 10px; width: 95%; border-radius: 10px; margin-left: 30px">

                <tr>
                    <td style="padding-left:10px"><br />&nbsp; ID <br /><br /></td>
                    <td><br />&nbsp; Name <br /><br /></td>
                    <td><br />&nbsp; E_mail <br /><br /></td>
                    <td><br />&nbsp; Subject <br /><br /></td>
                    <td><br /> &nbsp; Message <br /><br /> </td>
                </tr>

                <tbody>

                    @foreach (var s in Model)
                    {
                        <tr style="border:1px solid black">

                            <td style="padding:10px;padding-left:10px"> @s.contacter_Id </td>
                            <td style="padding:10px"> @s.con_name</td>
                            <td style="padding:10px"> @s.email</td>
                            <td style="padding:10px"> @s.subject</td>
                            <td style="padding:10px"> @s.message</td>
                           
                        </tr>
                    }


                </tbody>
            </table>
        </div>
        <!---->
        <br /><br />
    </div>





    <script src="~/js/bootstrap.js" type="text/javascript"></script>

</body>
</html>
